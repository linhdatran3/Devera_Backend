{
  "kind": "collectionType",
  "collectionName": "accounts",
  "info": {
    "name": "Account",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "walletAddress": {
      "type": "string",
      "required": true,
      "unique":true
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "role": {
      "type": "string",
      "default": "customer",
      "required": true
    },
    "users_permissions_role": {
      "plugin": "users-permissions",
      "model": "role",
      "default": 6
    }
  }
}
