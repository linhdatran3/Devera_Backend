{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "default": "Empty"
    },
    "price": {
      "type": "float",
      "default": 0,
      "required": true
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "isValidate": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "categories": {
      "collection": "category",
      "via": "products",
      "dominant": true
    },
    "typePassword": {
      "type": "password"
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
